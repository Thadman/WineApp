<h2 class="text-center">Welcome <%= current_user.name %></h2>

<div class="">
  <%= link_to "Delete Your Account", listing_path(@current_user.id), method: :delete %>
</div>

<div class="row justify-content-around">
  <%= @listing.each do |listing| %>
    <%= render 'card', listing: listing %>
  <% end %>
</div>

<%= will_paginate @listing %>